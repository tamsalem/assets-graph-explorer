{
  "name": "graph-explorer",
  "private": true,
  "version": "0.0.4",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && tsc -p electron/tsconfig.json && vite build && node -e \"require('fs').writeFileSync('dist-electron/package.json', '{\\\"type\\\":\\\"commonjs\\\"}')\"",
    "build:mac": "npm run build && electron-builder --mac",
    "debug-build": "npm run build && DEBUG=electron-builder* electron-builder --mac",
    "preview": "vite preview",
    "pretest": "tsc && vite build",
    "test": "vitest run",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none vite\" \"wait-on http://localhost:5173 && tsc -p electron/tsconfig.json && node -e \\\"require('fs').writeFileSync('dist-electron/package.json', '{\\\\\\\"type\\\\\\\":\\\\\\\"commonjs\\\\\\\"}')\\\" && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .\"",
    "release": "npm run build && electron-builder --mac --publish=always"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/cytoscape": "^3.31.0",
    "@types/papaparse": "^5.5.2",
    "autoprefixer": "^10.4.23",
    "cytoscape": "^3.33.1",
    "cytoscape-dagre": "^2.5.0",
    "cytoscape-fcose": "^2.2.0",
    "electron-squirrel-startup": "^1.0.1",
    "papaparse": "^5.5.3",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-cytoscapejs": "^2.0.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "wait-on": "^9.0.3"
  }
}
